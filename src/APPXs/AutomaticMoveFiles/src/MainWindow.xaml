<Window x:Class="GSA.ToolKits.AutomaticMoveFiles.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GSA.ToolKits.AutomaticMoveFiles"
        mc:Ignorable="d"
        Icon="/form.ico"
        WindowStartupLocation="CenterScreen"
        WindowState="Normal"
        Height="765" Width="900"
        KeyUp="Window_KeyUp" ResizeMode="CanMinimize">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="622*"/>
            <ColumnDefinition Width="15*"/>
            <ColumnDefinition Width="145*"/>
            <ColumnDefinition Width="19*"/>
        </Grid.ColumnDefinitions>
        <Border x:Name="MainContent" Grid.ColumnSpan="4">
            <Label Content="加载中，请稍候..." HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Microsoft YaHei" FontSize="18" />
        </Border>
    </Grid>
</Window>